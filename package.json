{
  "name": "npm-package-boilerplate",
  "version": "1.0.0",
  "description": "basic boilerplate for publish you npm package",
  "keywords": [
    "npm",
    "package",
    "boilerplate"
  ],
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "module": "dist/index.m.js",
  "source": "src/index.js",
  "scripts": {
    "build": "microbundle",
    "dev": "microbundle watch",
    "demo": "./node_modules/.bin/parcel example/index.html",
    "test": "jest",
    "prepare": "npm test && npm run build"
  },
  "publishConfig": {
    "registry": "public"
  },
  "author": "Tang Rui <ruitang307@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^23.6.0",
    "microbundle": "^0.6.0",
    "parcel-bundler": "^1.10.3",
    "regenerator-runtime": "^0.12.1"
  }
}
